  object ibIZM_STAT: TIBCSQL
    Connection = dmOne.IBDBOne
    Transaction = dmOne.trWrite
    SQL.Strings = (
      
        'execute block (STAT_OLD integer=:STAT_OLD, STAT_NEW integer=:STA' +
        'T_NEW, NOM integer=:NOM,'
      'KOD integer=:KOD, ID integer=:ID, KOD_OP integer=:KOD_OP)'
      'as'
      'declare variable DATE_VVOD date;'
      'declare variable N_STAT1 char(15);'
      'declare variable N_STAT2 char(15);'
      'begin'
      '    SELECT CURRENT_DATE FROM RDB$DATABASE into :DATE_VVOD;'
      ''
      '    update shethik s set s.stat_omb_f=:STAT_NEW'
      '    where s.nom_abon=:NOM and s.kod_sht=:KOD and s.id=:id;'
      ''
      'if (STAT_OLD=0) then'
      'begin'
      'N_STAT1='#39#1088#1086#1076#1085#1086#1081#39';'
      'end'
      ''
      'if (STAT_OLD=1) then'
      'begin'
      'N_STAT1='#39#1086#1073#1084'.'#1092#1086#1085#1076#39';'
      'end'
      ''
      'if (STAT_NEW=0) then'
      'begin'
      'N_STAT2='#39#1088#1086#1076#1085#1086#1081#39';'
      'end'
      ''
      'if (STAT_NEW=1) then'
      'begin'
      'N_STAT2='#39#1086#1073#1084'.'#1092#1086#1085#1076#39';'
      'end'
      '    '
      
        'INSERT INTO TMP_GURNAL_IZM (KOD_IZM, DATE_IZM, KOD_OP, NOM_ABON,' +
        ' DO_IZM, POSLE_IZM) VALUES'
      '('#39'SHETHIK'#39',:DATE_VVOD,:KOD_OP,:NOM,:N_STAT1,:N_STAT2);'
      ''
      'end')
    AutoCommit = True
    Left = 872
    Top = 112
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'STAT_OLD'
      end
      item
        DataType = ftUnknown

  object dsetSCH: TIBCQuery
    SQLInsert.Strings = (
      'INSERT INTO SHETHIK'
      
        '  (NOM_ABON, KOD_SHT, DATE_PLOMB, POK_UST, DATE_OTKL, STATUS, DA' +
        'TE_IZGOT, DATE_ZAM, DATE_UST, DATE_NAH_POV, DATE_POVR, STAT_OMB_' +
        'F, DATE_KON_POV, RAZRJD, ID)'
      'VALUES'
      
        '  (:NOM_ABON, :KOD_SHT, :DATE_PLOMB, :POK_UST, :DATE_OTKL, :STAT' +
        'US, :DATE_IZGOT, :DATE_ZAM, :DATE_UST, :DATE_NAH_POV, :DATE_POVR' +
        ', :STAT_OMB_F, :DATE_KON_POV, :RAZRJD, :ID)')
    SQLDelete.Strings = (
      'DELETE FROM SHETHIK'
      'WHERE'
      '  NOM_ABON = :Old_NOM_ABON AND ID = :Old_ID')
    SQLUpdate.Strings = (
      'UPDATE SHETHIK'
      'SET'
      
        '  NOM_ABON = :NOM_ABON, KOD_SHT = :KOD_SHT, DATE_PLOMB = :DATE_P' +
        'LOMB, POK_UST = :POK_UST, DATE_OTKL = :DATE_OTKL, STATUS = :STAT' +
        'US, DATE_IZGOT = :DATE_IZGOT, DATE_ZAM = :DATE_ZAM, DATE_UST = :' +
        'DATE_UST, DATE_NAH_POV = :DATE_NAH_POV, DATE_POVR = :DATE_POVR, ' +
        'STAT_OMB_F = :STAT_OMB_F, DATE_KON_POV = :DATE_KON_POV, RAZRJD =' +
        ' :RAZRJD, ID = :ID'
      'WHERE'
      '  NOM_ABON = :Old_NOM_ABON AND ID = :Old_ID')
    SQLRefresh.Strings = (
      
        'SELECT NOM_ABON, KOD_SHT, DATE_PLOMB, POK_UST, DATE_OTKL, STATUS' +
        ', DATE_IZGOT, DATE_ZAM, DATE_UST, DATE_NAH_POV, DATE_POVR, STAT_' +
        'OMB_F, DATE_KON_POV, RAZRJD, ID, magn_plomb FROM SHETHIK'
      'WHERE'
      '  NOM_ABON = :NOM_ABON AND ID = :ID')
    SQLLock.Strings = (
      'SELECT NULL FROM SHETHIK'
      'WHERE'
      'NOM_ABON = :Old_NOM_ABON AND ID = :Old_ID'
      'FOR UPDATE WITH LOCK')
    SQLRecCount.Strings = (
      'SELECT COUNT(*) FROM ('
      'SELECT 1 AS C  FROM SHETHIK'
      ''
      ') q')
    Connection = dmOne.IBDBOne
    SQL.Strings = (
      
        'select f.id, sa.datadog, f.razrjd, F.kod_sht AS KOD_PRIB,F.DATE_' +
        'IZGOT, F.NOM_ABON,SA.FIO, SA.DOM,sa.DOM2,SA.KB,'
      
        'sa.KORPUS,  SP.NAI_PUNKT, VU.NAI_VU,SU.NAI_UL,(select pc.PHONE f' +
        'rom pc_phone(sa.nom_abon) pc) as PHONE,'
      
        'SA.KWMETR, SVP.NAI_VIDPR , SK.MARKA, SK.MODEL, F.DATE_UST, F.DAT' +
        'E_OTKL,  F.STATUS ,SK.VID_PRIB , SVP.KLASS,'
      
        '(select kl.naim_kod from klsfct kl where kl.kod_hc = f.teh_obsl ' +
        'and kl.kod = 13) as TEH_OBLUG ,TRIM(F.PLOMB) as PLOMB,F.DATE_PLO' +
        'MB,'
      
        'F.DATE_POVR,iif(f.pok_pov is not null,'#39'0'#39','#39'1'#39') as N_POV, F.DATE_' +
        'NAH_POV, F.DATE_KON_POV, F.date_ust, F.DATE_ZAM,F.POK_UST,'
      'iif(F.ZAV_NOM is null,'#39#39',trim(F.ZAV_NOM)) as ZAV_NOM,'
      'l.kon_pok, l.pok_usl,'
      
        '(select k.naim_kod from klsfct k where k.kod=40 and k.kod_hc=f.k' +
        'em_plomb) as KEM_OPLOMB,'
      'f.stat_omb_f, iif(f.stat_omb_f=1,'#39#1089#1095'.'#1086#1073#1084'.'#1092'.'#39','#39#39') as N_SCH,'
      
        '(select count(*) from shethik h where h.nom_abon=f.nom_abon and ' +
        'coalesce(h.pr_zamena,0)=0) as KOL_SCH,'
      
        '(select iif(coalesce(l.status_pov,0)!=0,'#39#1085#1072' '#1087#1086#1074#1077#1088#1082#1077#39','#39#39') from li' +
        'cev l where l.nom_abon=f.nom_abon) as POV,'
      
        '(select sotr.fio_kontr from sp_kontr sotr where sotr.kod_kontr=s' +
        'a.kmaster),'
      
        '(select k.naim_kod from klsfct k, sp_podvodka sp where k.kod=85 ' +
        'and k.kod_hc=sp.kod_vid and sp.kod_podv=f.kod_podv) as VID_PODV,'
      
        '(select kl.naim_kod from klsfct kl where kl.kod_hc = f.teh_obsl ' +
        'and kl.kod = 13) as TEH_OBLUG,'
      
        'iif(sa.kod_geu=1002 or sa.kod_geu=2001, '#39#1095#1072#1089#1090#1085#1099#1081' '#1089#1077#1082#1090#1086#1088#39', '#39#1084#1085#1086#1075#1086 +
        #1101#1090#1072#1078#1082#1072#39') as mnogoet,'
      
        'iif(sa.kod_gaz=1,'#39#1087#1088#1080#1088#1086#1076#1085#1099#1081#39','#39#1089#1078#1080#1078#1077#1085#1085#1099#1081#39') as VID_GAZA, sk.izgoto' +
        'v,'
      
        '(select ks.naim_kod from klsfct ks where ks.kod=8 and ks.kod_hc=' +
